{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es2015",
        "outDir": "out",
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "rootDir": ".",
        "baseUrl": "./node_modules",
        "paths": {
            "qunit": ["./node_modules/retyped-qunit-tsd-ambient/qunit.d.ts"] 
        }
    },
    "exclude": [
        "lib",
        "node_modules"
    ]
}